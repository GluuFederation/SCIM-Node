{
  "name": "scim-node-2",
  "version": "2.1.0",
  "description": "node lib for SCIM 2.0",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "author": {
    "name": "meghna joshi"
  },
  "bugs": {
    "url": "https://github.com/GluuFederation/scim-node/issues"
  },
  "scripts": {
    "start": "node -r source-map-support/register build/index.js",
    "debug": "tsc && node -r source-map-support/register %NODE_DEBUG_OPTION% build/test.js",
    "test": "jest",
    "build": "rm -rf build && tsc",
    "test-CI": "jest --ci --coverage",
    "git-add-builded": "git add build",
    "watch": "jest --env=jsdom --watch"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/GluuFederation/scim-node"
  },
  "pre-commit": [
    "build",
    "git-add-builded"
  ],
  "license": "MIT",
  "deprecated": false,
  "homepage": "https://github.com/GluuFederation/scim-node#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.4.7",
    "jsonwebtoken": "^8.3.0",
    "source-map-support": "^0.5.9",
    "uuid": "^3.3.2",
    "www-authenticate": "^0.6.2"
  },
  "devDependencies": {
    "typescript": "^3.0.3",
    "@types/uuid": "^3.4.4",
    "@types/node": "^10.10.1",
    "babel-eslint": "^9.0.0",
    "dts-generator": "^2.1.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb-es5": "^1.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "pre-commit": "^1.2.2",
    "ts-jest": "^23.1.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
